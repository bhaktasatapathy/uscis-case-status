{
	"name": "USCIS Status Checker",
	"version": "1.0",
	"description": "USCIS Status Checker",
	"permissions": [
		"storage",
		"https://*/",
		"notifications",
		"alarms"
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "USCIS Case Tracker"
	},
	"manifest_version": 2
}